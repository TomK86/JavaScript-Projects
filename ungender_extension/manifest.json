{
"manifest_version": 2,
    "name": "Un-gender",
    "description": "Replaces all gendered pronouns with gender-neutral pronouns.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}